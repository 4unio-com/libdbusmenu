<?xml version='1.0' encoding='UTF-8' ?>

<account version='1.0'>
	<account>
		<protocol>{prpl_protocol}</protocol>
		<name>{username_and_domain}</name>
		<password>{password}</password>
		<alias>{alias}</alias>
		<settings ui='gtk-gaim'>
			<setting name='auto-login' type='bool'>1</setting>
		</settings>
		<current_error/>
	</account>
</account>
